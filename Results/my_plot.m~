function[] = my_plot(name, nek, val, x, y, xlab, ylab, ls, colour)

    figure('Name', name)
    hold on 
    plot(nek.(x), nek.(y), ls.nek, 'DisplayName', 'Nektar++ Results')
    plot(val.ccm_sst.(x), val.ccm_sst.(y), ls.sst, 'LineWidth', 2, 'Color', colour.sst, 'DisplayName', 'Star SST')
    plot(val.ccm_sa.(x), val.ccm_sa.(y), ls.sa, 'Color', colour.sa, 'DisplayName', 'Star SA')
    plot(val.exp.(x), val.exp.(y), ls.exp, 'DisplayName', 'Experimental')
    hold off
    xlabel(xlab)
    ylabel(ylab)
    grid on
    grid minor
    legend('Location','NorthWest')

end